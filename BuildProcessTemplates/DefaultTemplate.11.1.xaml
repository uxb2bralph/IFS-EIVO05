        <Assign x:TypeArguments="x:String" DisplayName="初始化原始檔目錄" To="[SourcesDirectory]" Value="[String.Format(&quot;{0}\Sources&quot;, BuildDirectory)]" mtbwt:BuildTrackingParticipant.Importance="Low"/>
